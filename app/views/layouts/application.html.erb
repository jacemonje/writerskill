<!DOCTYPE html>
<html>
<head>
  <title>Writerskill</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <br>

<center>
  <p class="notice"><%= notice %></p>
  <p class="alert"><%= alert %></p>
</center>


  <div align = "right">

    <% if member_signed_in? %>

     <%= link_to('Dashboard', :controller => "dashboard", :action => "index") %>
     <%= link_to('Events', :controller => "events", :action => "index") %>
     Work
     <%= link_to('Logout', destroy_member_session_path, :method => :delete) %>


  <% else %>
    <%= link_to('Home', root_path) %> |
    <%= link_to('Sign up', new_member_registration_path ) %> |
    <%= link_to('Login?', new_member_session_path)  %>
  <% end %>

  </div>




<%= yield %>

</body>


<footer>
  <br>

    <center>
    Writerskill Member System, All Rights Reserved or Whatever
  </center>

</footer>


</html>
