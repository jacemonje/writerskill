<p id="notice"><%= notice %></p>

<center>
  <%= image_tag("logo.jpg") %>
<h1>Events</h1>
<center>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Venue</th>
      <th>Organized By</th>
      <th>Schedule</th>
      <th>Type</th>
      <th>Attendance</th>

      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= link_to event.name, event %></td>
        <td><%= event.venue %></td>
        <td><%= event.organized_by %></td>
        <td><%= event.schedule.to_formatted_s(:long_ordinal)  %></td>
        <td><%= event.eventclass %></td>
        <td><%= event.attendance %></td>



      </tr>
    <% end %>
  </tbody>
</table>

<br>
